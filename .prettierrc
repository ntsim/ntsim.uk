{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "plugins": [
    "prettier-plugin-tailwindcss",
    "prettier-plugin-classnames",
    "prettier-plugin-astro",
    "prettier-plugin-merge"
  ],
  "tailwindStylesheet": "./src/styles/index.css",
  "overrides": [
    {
      "files": "*.astro",
      "options": {
        "parser": "astro",
        "astroAllowShorthand": true
      }
    }
  ]
}
